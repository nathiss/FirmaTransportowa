# EditorConfig is awesome: https://EditorConfig.org

root = true


[*.py]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4


[*.js]
indent_style = space
indent_size = 2


[{.gitignore, README.md}]
insert_final_newline = false


[{requirements.txt, requirements-dev.txt,}]
insert_final_newline = true